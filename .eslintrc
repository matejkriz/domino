{
    "env": {
      "node": true
    }
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "no-console": 0,
        "no-use-before-define": [1, "nofunc"]
    },
    "globals": {
        "global": true,
        "process": true,
        "require": true,
        "define": false
    }
}
